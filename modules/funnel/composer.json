{
    "name": "ercee/module-funnel",
    "description": "Marketing automation funnel module for Ercee CMS",
    "version": "1.0.0",
    "type": "ercee-module",
    "license": "proprietary",
    "require": {
        "php": "^8.2",
        "filament/filament": "^3.2",
        "laravel/framework": "^12.0",
        "ercee/module-forms": "^1.0",
        "ercee/module-commerce": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Modules\\Funnel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Funnel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "ercee": {
            "provider": "Modules\\Funnel\\FunnelModuleServiceProvider"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
